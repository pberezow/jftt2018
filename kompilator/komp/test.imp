DECLARE
    n; j; sito(2:3000000000000);
IN
    FOR i FROM 3 DOWNTO 0 DO
        sito(3000000000000) := 10;
        FOR k FROM 1 TO 2 DO
            WRITE k;
        ENDFOR
        FOR k FROM 3 TO 4 DO
            WRITE k;
        ENDFOR
        READ k;
        WRITE sito(j);
        WRITE 11111111;
    ENDFOR
END
